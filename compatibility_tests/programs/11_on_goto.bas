1 REM *** ON GOTO/GOSUB TEST ***
10 PRINT "=== ON GOTO/GOSUB ==="
20 FOR X=1 TO 4
30 PRINT "X=";X;": ";
40 ON X GOTO 100,110,120,130
50 PRINT "FELL THROUGH"
60 GOTO 140
100 PRINT "CASE 1":GOTO 140
110 PRINT "CASE 2":GOTO 140
120 PRINT "CASE 3":GOTO 140
130 PRINT "CASE 4":GOTO 140
140 NEXT X
150 PRINT "ON GOSUB TEST:"
160 FOR X=1 TO 3
170 PRINT "X=";X;": ";
180 ON X GOSUB 300,310,320
190 NEXT X
200 PRINT "=== END ON GOTO/GOSUB ==="
210 END
300 PRINT "SUB 1":RETURN
310 PRINT "SUB 2":RETURN
320 PRINT "SUB 3":RETURN
