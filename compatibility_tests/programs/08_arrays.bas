1 REM *** ARRAY TEST ***
10 PRINT "=== ARRAYS ==="
20 DIM A(10)
30 FOR I=0 TO 10
40 A(I)=I*I
50 NEXT I
60 PRINT "A(0)=";A(0)
70 PRINT "A(5)=";A(5)
80 PRINT "A(10)=";A(10)
90 DIM B(3,3)
100 FOR I=0 TO 3
110 FOR J=0 TO 3
120 B(I,J)=I*10+J
130 NEXT J
140 NEXT I
150 PRINT "B(0,0)=";B(0,0)
160 PRINT "B(2,3)=";B(2,3)
170 PRINT "B(3,3)=";B(3,3)
180 DIM C$(5)
190 C$(1)="ONE"
200 C$(2)="TWO"
210 C$(3)="THREE"
220 PRINT "C$(1)=";C$(1)
230 PRINT "C$(2)=";C$(2)
240 PRINT "C$(3)=";C$(3)
250 PRINT "=== END ARRAYS ==="
260 END
